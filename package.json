{
  "name": "clean-start-typescript",
  "version": "1.0.0",
  "description": "webpack, babel, react, typescript, express, and sass.",
  "main": "service/index.js",
  "scripts": {
    "start": "node ./service/index.js",
    "start:dev": "webpack serve --mode development",
    "dev": "webpack --mode development --progress",
    "build": "webpack --mode production --progress",
    "test": "mocha --require @babel/register --require test/helper.js -c test/*Spec.js",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/clean-start-typescript.git"
  },
  "keywords": [
    "react",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "bin": {
    "clean-start-typescript": "./bin/csr.js"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-remove-template-literals-whitespace": "^1.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "copy-webpack-plugin": "^12.0.2",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^7.1.1",
    "express-cache-headers": "^0.1.4",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "node-sass": "^9.0.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
