    "react-router": "^6.23.0",
    "react-router-dom": "^6.23.0"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/clean-start-typescript/issues"
  },
  "homepage": "http://www.augmentedjs.com"
